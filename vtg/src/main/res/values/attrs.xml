<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="fontStyle">
        <attr name="typeface" />
    </declare-styleable>


    <attr name="typeface" format="enum">
        <enum name="regular" value="1" />
        <enum name="bold" value="2" />
    </attr>

    <declare-styleable name="ReadMoreTextView">
        <attr name="readMoreMaxLine" format="integer" />
        <attr name="readMoreText" format="string|reference" />
        <attr name="readMoreColor" format="color|reference" />
    </declare-styleable>

        <declare-styleable name="vaccincard">

            <attr name="cardBackground" format="reference" />
            <attr name="person_pic" format="string" />
            <attr name="cardNumber" format="string" />
            <attr name="textColor" format="color" />
        </declare-styleable>

</resources>